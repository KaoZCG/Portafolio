{
  "version": 2,
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "routes": [
    {
      "src": "/build/(.*)",
      "dest": "/public/build/$1",
      "continue": true
    },
    {
      "src": "/(.*\\.(ico|png|jpg|jpeg|gif|svg|css|js|json|txt|xml|webmanifest))",
      "dest": "/public/$1",
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.php",
      "continue": true
    }
  ]
}
{
  "version": 2,
  "builds": [
    {
      "src": "Dockerfile",
      "use": "docker"
    }
  ],
  "routes": [
    {
      "src": "/build/(.*)",
      "dest": "/public/build/$1"
    },
    {
      "src": "/(.*\\.(ico|png|jpg|jpeg|gif|svg|css|js|json|txt|xml|webmanifest))",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.php"
    }
  ]
}